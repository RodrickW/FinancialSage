{
  "name": "MindMyMoneyMobile",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build-android": "cd android && ./gradlew assembleRelease",
    "build-ios": "cd ios && xcodebuild -workspace MindMyMoneyMobile.xcworkspace -scheme MindMyMoneyMobile -configuration Release clean build",
    "bundle-android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "clean": "cd android && ./gradlew clean && cd ../ios && xcodebuild clean",
    "pod-install": "cd ios && pod install"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-native": "0.74.0",
    "@react-native-community/cli": "^12.0.0",
    "react-native-vector-icons": "^10.1.0",
    "react-native-svg": "^15.3.0",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native-stack": "^6.9.26",
    "react-native-screens": "^3.32.0",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-gesture-handler": "^2.17.1",
    "react-native-reanimated": "^3.11.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-chart-kit": "^6.12.0",
    "react-native-progress": "^5.0.1",
    "react-native-elements": "^3.4.3",
    "react-native-paper": "^5.12.3",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "react-native-keychain": "^8.2.0",
    "react-native-device-info": "^10.13.0",
    "react-native-biometrics": "^3.0.1",
    "react-native-plaid-link-sdk": "^11.5.0",
    "react-native-webview": "^13.10.5",
    "@tanstack/react-query": "^5.50.0",
    "axios": "^1.7.2",
    "date-fns": "^3.6.0",
    "react-hook-form": "^7.52.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/runtime": "^7.24.0",
    "@react-native/babel-preset": "0.74.87",
    "@react-native/eslint-config": "^0.74.87",
    "@react-native/metro-config": "^0.74.87",
    "@react-native/typescript-config": "^0.74.87",
    "@tsconfig/react-native": "^3.0.5",
    "@types/react": "^18.3.3",
    "@types/react-test-renderer": "^18.3.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.4.5"
  },
  "jest": {
    "preset": "react-native"
  },
  "engines": {
    "node": ">=18"
  }
}